{
    "manifest_version": 2,
    "name": "CZ - DJ for Google Chrome",
    "version": "0.1",
    "icons": {
      "16": "New 16.png",
      "48": "New 48.png",
      "128": "New 128.png"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "CZ"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "commands": {
      "_execute_browser_action": {
        "suggested_key": {
          "default": "Ctrl+M",
          "mac": "MacCtrl+M"
        },
        "description": "Opens popup.html"
      }
    }
  }
  